<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <include relativeToChangelogFile="false" file="classpath*:liquibase-changeLog-inventory.xml"/>
    <include relativeToChangelogFile="false" file="classpath*:liquibase-changeLog-monitoring.xml"/>
    <include relativeToChangelogFile="false" file="classpath*:liquibase-changeLog-missionControl.xml"/>
    <include relativeToChangelogFile="false" file="classpath*:liquibase-changeLog-carePlanning.xml"/>

</databaseChangeLog>
